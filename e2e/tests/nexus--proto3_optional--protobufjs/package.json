{
  "name": "@proto-graphql/e2e-nexus--proto3_optional--protobufjs",
  "version": "0.0.0",
  "description": "E2E tests for protoc-gen-nexus",
  "private": true,
  "scripts": {
    "test:e2e:prepare": "pnpm run test:e2e:gen:proto && pnpm run test:e2e:gen:types",
    "test:e2e:gen:proto": "rm -rf __generated__/schema && buf generate --template buf.gen.json",
    "test:e2e:gen:types": "rm -rf __generated__/typings.ts && tsx schema.ts",
    "test:e2e": "vitest run --passWithNoTests --typecheck --config ../../vitest.config.ts"
  },
  "dependencies": {
    "nexus": "catalog:test",
    "protobufjs": "catalog:test",
    "graphql": "catalog:test",
    "graphql-scalars": "catalog:test"
  },
  "devDependencies": {
    "@proto-graphql/e2e-helper": "workspace:*",
    "@proto-graphql/e2e-testapis-protobufjs": "workspace:*",
    "@proto-graphql/tsconfig": "workspace:*",
    "@proto-nexus/protobufjs": "workspace:*",
    "proto-nexus": "workspace:*",
    "protoc-gen-nexus": "workspace:*"
  }
}
