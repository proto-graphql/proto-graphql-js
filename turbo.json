{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "lint": {},
    "test": {
      "dependsOn": ["^build", "^build:test"]
    },
    "//#gen:testapis": {
      "cache": false
    },
    "build:test": {
      "dependsOn": ["//#gen:testapis"]
    }
  }
}
