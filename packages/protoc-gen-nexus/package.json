{
  "name": "protoc-gen-nexus",
  "version": "0.5.4",
  "description": "Generate DSL for GraphQL Nexus from Protocol Buffers IDL",
  "keywords": [
    "graphql",
    "grpc",
    "nexus",
    "protobuf",
    "typescript"
  ],
  "bin": {
    "protoc-gen-nexus": "./bin/protoc-gen-nexus"
  },
  "repository": "git@github.com:proto-graphql/proto-graphql-js.git",
  "author": "izumin5210 <m@izum.in>",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./module/index.js",
  "types": "./lib/index.d.ts",
  "private": false,
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "@proto-graphql/codegen-core": "^0.2.0",
    "@proto-graphql/proto-descriptors": "^0.1.0",
    "@proto-graphql/protoc-plugin-helpers": "^0.2.0",
    "change-case": "^4.1.2",
    "google-protobuf": "^3.20.1"
  },
  "devDependencies": {
    "@proto-nexus/google-protobuf": "^0.5.1",
    "@proto-nexus/protobufjs": "^0.5.1",
    "@testapis/node": "^0.5.0",
    "@testapis/node-native": "^0.5.1",
    "@types/glob": "^8.0.0",
    "glob": "^8.0.1",
    "nexus": "^1.0.0"
  },
  "files": [
    "bin/",
    "include/",
    "lib/",
    "module/"
  ],
  "scripts": {
    "build": "tsc --build . && tsc --build tsconfig.module.json",
    "clean": "rimraf lib/ module/ && tsc --build . --clean && tsc --build tsconfig.module.json --clean",
    "test": "jest --coverage"
  }
}
