{
  "name": "proto-nexus",
  "version": "0.5.1",
  "description": "Generate DSL for GraphQL Nexus from Protocol Buffers IDL",
  "keywords": [
    "graphql",
    "grpc",
    "nexus",
    "protobuf",
    "typescript"
  ],
  "repository": "git@github.com:proto-graphql/proto-graphql-js.git",
  "author": "izumin5210 <m@izum.in>",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./module/index.js",
  "types": "./lib/index.d.ts",
  "private": false,
  "engines": {
    "node": ">= 14.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/",
    "module/"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:module",
    "build:cjs": "tsc --build tsconfig.cjs.json",
    "build:module": "tsc --build tsconfig.module.json",
    "clean": "yarn clean:cjs && yarn clean:module",
    "clean:cjs": "yarn build:cjs --clean",
    "clean:module": "yarn build:module --clean",
    "lint": "eslint --fix ."
  },
  "devDependencies": {
    "@proto-graphql/eslint-config": "*",
    "@proto-graphql/tsconfig": "*"
  }
}
