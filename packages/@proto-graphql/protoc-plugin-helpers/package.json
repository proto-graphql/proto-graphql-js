{
  "name": "@proto-graphql/protoc-plugin-helpers",
  "version": "0.4.2",
  "description": "",
  "keywords": [
    "graphql",
    "grpc",
    "nexus",
    "protobuf",
    "typescript"
  ],
  "repository": "git@github.com:proto-graphql/proto-graphql-js.git",
  "author": "izumin5210 <m@izum.in>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "src/",
    "dist/",
    "!src/**/*.test.ts",
    "!src/**/__tests__/"
  ],
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "catalog:",
    "@bufbuild/protoplugin": "catalog:",
    "@proto-graphql/codegen-core": "workspace:*"
  },
  "devDependencies": {
    "@proto-graphql/tsconfig": "workspace:*"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run --passWithNoTests"
  }
}
