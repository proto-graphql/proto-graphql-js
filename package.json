{
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/@proto-graphql/*",
    "packages/@proto-nexus/*",
    "packages/@testapis/*",
    "e2e"
  ],
  "scripts": {
    "build": "turbo run build",
    "generate:testapis": "./scripts/compile-testapis-proto",
    "format": "prettier --write '**/*.{mjs,js,jsx,mts,ts,tsx}'",
    "lint": "eslint . --fix",
    "test": "turbo run test",
    "test:e2e": "yarn generate:testapis && turbo run test:e2e",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "release": "yarn build && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.7",
    "@changesets/cli": "^2.25.0",
    "@types/google-protobuf": "3.15.6",
    "@types/jest": "29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-unused-imports": "^2.0.0",
    "google-protobuf": "3.21.2",
    "graphql": "15.4.0",
    "graphql-scalars": "1.20.1",
    "husky": "^8.0.2",
    "jest": "29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "protobufjs": "6.11.3",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "ts-proto": "^1.129.0",
    "ts-protoc-gen": "0.15.0",
    "turbo": "^1.6.1",
    "typescript": "4.9.4"
  },
  "changelog": {
    "repo": "proto-graphql/proto-graphql-js",
    "labels": {
      "codegen - breaking changes :warning:": "⚠️ Breaking Changes - Code Generation",
      "codegen - feature :genie_woman:": "Features - Code Generation",
      "codegen - improvement :vulcan_salute:": "Improvements - Code Generation",
      "runtime - improvement :running_man:": "Improvements - Runtime",
      "bug :bug:": "Bug Fixes",
      "dependencies": "Dependencies"
    }
  },
  "name": "proto-nexus"
}
